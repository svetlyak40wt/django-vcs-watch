#, python-format
msgid "Latest changes at %s"
msgstr "Последние изменения с %s"

#: models.py:51
#, python-format
msgid "Commit %(revision)s by %(author)s"
msgstr "Коммит %(revision)s от %(author)s"

#: models.py:72
#, python-format
msgid "Repository at %s"
